{
    "name": "Adblocker",
    "version": "1.0.0",
    "description": "blocks the ads",
    "permissions": [
        "webRequest"
    ,"webRequestBlocking"
    ,"*://*.google.com/*"
     ],
    "background":{
        "scripts": ["background.js"]
    },
    "icons":{
        "16":"./icons/icon16.svg",
        "32":"./icons/icon32.svg",
        "48": "./icons/icon48.svg",
        "128": "./icons/icon128.svg"
    },
    "manifest_version": 3
}
